

<div class="container">
    <div class="card">
        <div class="card-header">
            <h1 class="h5">Pedidos</h1>
        </div>
        <div class="card-body">
            <table>
                <thead>
                    <tr>
                        <th></th>
                        <th>Local:</th>
                        <th>Data:</th>
                    </tr>
                </thead>
                <tbody>
                    <% tanques.forEach(tanques => { %>
                    <tr>
                        <td><img src="imgs/order.png" alt="Pedido"></td>
                        <td><%= tanques.local %></td>
                        <td><%= tanques.data %></td>
                    </tr>
                    <% }) %>
                </tbody>
            </table>
        </div>
    </div>
</div>